#a simple calculator that can do basic math, nothing too fancy.

number1= int(input("number"))
number2= int(input("second number"))
a= input("operator")


if a == "+":
  print (float(number1) + float(number2))

if a =="-":
  print (float(number1) - float(number2))

if a == "*":
  print ((number1) * (number2))

if a == "/":
  print ((number1) / (number2))


